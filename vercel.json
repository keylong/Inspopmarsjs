{
  "version": 2,
  "name": "inspopmars-instagram-downloader",
  "alias": ["ins.popmars.com", "www.ins.popmars.com"],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_SITE_URL": "https://ins.popmars.com",
    "NEXT_PUBLIC_APP_NAME": "Instagram Downloader",
    "NEXT_PUBLIC_APP_DESCRIPTION": "Free Instagram Photo, Video, Story, IGTV, Highlights Downloader",
    "NEXT_PUBLIC_GOOGLE_ANALYTICS_ID": "@google_analytics_id",
    "NEXT_PUBLIC_BAIDU_ANALYTICS_ID": "@baidu_analytics_id"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "pages/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://ins.popmars.com"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/(.*\\\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/www.ins.popmars.com/(.*)",
      "destination": "https://ins.popmars.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/robots.txt",
      "destination": "/api/robots"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/analytics",
      "schedule": "0 3 * * *"
    }
  ]
}